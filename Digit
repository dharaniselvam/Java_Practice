package Practice;
// returns number of digits in a number
import java.util.Scanner;

public class Digit {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n=s.nextInt();
		int digit = 0;
		while(n!=0) {
			n=n/10;
			digit++;
		}
		
		System.out.println(digit);

	}

}
