import java.util.Scanner;
// returnsthe maximum number in an array
public class Main {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		if(n%4==0)
		    if(n%100==0)
		        if(n%400==0)
		            System.out.print(n + " is a leap year");
	            else
	                System.out.print(n + " is not a leap year");
            else
                System.out.print(n + " is a leap year");
        else
            System.out.print(n + " is not a leap year");
}
}
