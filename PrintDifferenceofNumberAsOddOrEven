import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        int a = s.nextInt();
        int diff = n-a;
        if(diff%2==0)
            System.out.println("Even");
        else
            System.out.println("Even");

        
    }
}
