import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        int m = s.nextInt();
        int mul = n*m;
        if(mul==0)
            System.out.println("Even");
        else 
            System.out.println("Odd");

    }
}
