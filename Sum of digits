import java.util.Scanner;
//returns sum of digits
public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();//97
        int sum=0,r=0;
        while(n>0){
            r=n%10;
            sum = sum+r;
            n=n/10;
        }
        System.out.print(sum);
        
}
}
